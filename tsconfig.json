{
   "compilerOptions": {
     "target": "ES2020",
     "forceConsistentCasingInFileNames": true,
     "useDefineForClassFields": true,
     "lib": ["ES2020", "DOM", "DOM.Iterable"],
     "module": "ESNext",
     "skipLibCheck": true,
     "moduleResolution": "bundler",
     "allowImportingTsExtensions": true,
     "resolveJsonModule": true,
     "isolatedModules": true,
     "noEmit": true,
     "jsx": "react-jsx",
     "strict": true,
     "noUnusedLocals": true,
     "noUnusedParameters": true,
     "noFallthroughCasesInSwitch": true,
     "baseUrl": ".",
     "paths": {
       "@/*": ["./src/*"],
       "@components/*": ["src/components/*"],
       "@pages/*": ["src/pages/*"],
       "@styles/*": ["src/styles/*"],
       "@data/*": ["src/data/*"]
     },
     "typeRoots": [
       "./node_modules/@types",
       "./src/types"
     ]
   },
   "include": [
     "src/**/*.ts",
     "src/**/*.tsx",
     "src/**/*.d.ts"
, "src/components/Sound/SoundPlayer.tsx.bak", "src/components/Sound/AudioPlayer.tsx.bak", "src/components/BulkImagesLoader/BulkImageLoader-b4Logo.tsx.bak", "src/components/BulkImagesLoader/BulkImageLoader.tsx.bak"   ],
   "references": [{ "path": "./tsconfig.node.json" }]
 }